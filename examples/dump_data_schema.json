{
    "$schema": "http://json-schema.org/draft-07/schema",
    "args": {
        "type": "list",
        "description": "args of the function",
        "default": []
    },
    "kwargs": {
        "type": "dict",
        "description": "kwargs of the function",
        "default": {}
    },
    "result": {
        "type": "any",
        "description": "result(s) of the function"
    },
    "function": {
        "type": "function",
        "description": "Actual function to be called",
        "default": null
    },
    "module": {
        "type": "module",
        "description": "Module of the function, unused",
        "default": null
    },
    "description": {
        "type": "string",
        "description": "Description",
        "default": ""
    },
    "name": {
        "type": "string",
        "description": "Short name of the test, to be shown by runner; if not present, the filename instead",
        "default": ""
    }
}
